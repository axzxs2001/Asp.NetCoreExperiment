{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Urls": "http://*:5000",
  "UpgradeSetting": {
    //是否启用
    "Enable": true,
    //多少时间请求更新服务器
    "CronExpression": "*/30 * * * * ?",
    //请求的更新服务器地址
    "UpgradeServerUrl": "http://localhost:5555",
    //自己的服务器地址
    "SelfUrl": "http://127.0.0.1:5000",
    //更新文件的文件
    "ZipFile": "AutoUpdate_Demo.zip"
  }
}
