{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Urls": "http://*:5000",
  "UpgradeSetting": {
    "Enable": true,
    "CronExpression": "*/30 * * * * ?",
    "UpgradeServerUrl": "http://localhost:5555",
    "SelfUrl": "http://127.0.0.1:5000",
    "ZipFile": "AutoUpdate_Demo.zip"
  }
}
